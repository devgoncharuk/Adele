footer
    .container
        .footer
            .footer-menu
                -var footer__nav = ["about", "terms", "privacy", "contacts"];
                each nav in footer__nav
                    a(href="#")=nav
            ul.social-icons
                li
                    a.facebook(href="")
                        i.fa.fa-facebook(aria-hidden="true")
                li
                    a.twitter(href="")
                        i.fa.fa-twitter(aria-hidden="true")
                li
                    a.youtube(href="")
                        i.fa.fa-youtube-play(aria-hidden="true")
                li
                    a.dribbble(href="")
                        i.fa.fa-dribbble(aria-hidden="true")